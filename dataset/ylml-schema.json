{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://example.com/ylml-schema.json",
    "title": "苏州园林目录数据结构",
    "description": "苏州市园林局园林目录数据的JSON Schema定义",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "assessments": {
                "type": "object",
                "description": "园林评估信息",
                "properties": {
                    "basicCharacteristics": {
                        "oneOf": [
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "显著",
                                        "明显",
                                        "一般",
                                        "较弱"
                                    ]
                                }
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "基本特征评估"
                    },
                    "contemporaryCharacteristics": {
                        "oneOf": [
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "显著",
                                        "明显",
                                        "一般",
                                        "较弱"
                                    ]
                                }
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "当代特征评估"
                    },
                    "culturalTradition": {
                        "oneOf": [
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "融合",
                                        "显著",
                                        "明显",
                                        "一般",
                                        "较弱"
                                    ]
                                }
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "文化传统评估"
                    },
                    "featureProminence": {
                        "oneOf": [
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "显著",
                                        "明显",
                                        "一般",
                                        "较弱"
                                    ]
                                }
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "特色突出程度评估"
                    },
                    "modelRole": {
                        "oneOf": [
                            {
                                "type": "array",
                                "items": {
                                    "type": "string",
                                    "enum": [
                                        "显著",
                                        "明显",
                                        "一般",
                                        "较弱"
                                    ]
                                }
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "典型作用评估"
                    }
                },
                "required": [
                    "contemporaryCharacteristics",
                    "culturalTradition",
                    "featureProminence",
                    "modelRole"
                ],
                "additionalProperties": false
            },
            "basicInfo": {
                "type": "object",
                "description": "园林基本信息",
                "properties": {
                    "officialName": {
                        "type": "string",
                        "description": "园林官方名称"
                    },
                    "address": {
                        "type": "string",
                        "description": "园林地址"
                    },
                    "currentUse": {
                        "oneOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "当前用途"
                    },
                    "area": {
                        "oneOf": [
                            {
                                "type": "object",
                                "properties": {
                                    "value": {
                                        "type": "number",
                                        "minimum": 0,
                                        "description": "面积数值"
                                    },
                                    "unit": {
                                        "type": "string",
                                        "enum": [
                                            "平方米"
                                        ],
                                        "description": "面积单位"
                                    }
                                },
                                "required": [
                                    "value",
                                    "unit"
                                ],
                                "additionalProperties": false
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "园林面积"
                    },
                    "protectionStatus": {
                        "oneOf": [
                            {
                                "type": "string",
                                "enum": [
                                    "好",
                                    "中",
                                    "差"
                                ]
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "保护状态"
                    },
                    "openToPublic": {
                        "oneOf": [
                            {
                                "type": "boolean"
                            },
                            {
                                "type": "null"
                            }
                        ],
                        "description": "是否对公众开放"
                    }
                },
                "required": [
                    "officialName",
                    "address",
                    "currentUse",
                    "area",
                    "protectionStatus",
                    "openToPublic"
                ],
                "additionalProperties": false
            },
            "description": {
                "type": "string",
                "description": "园林详细描述"
            },
            "keyElements": {
                "oneOf": [
                    {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "enum": [
                                "建筑",
                                "花木",
                                "叠山",
                                "理水",
                                "其他"
                            ]
                        },
                        "uniqueItems": true
                    },
                    {
                        "type": "null"
                    }
                ],
                "description": "园林关键要素"
            },
            "media": {
                "type": "object",
                "description": "媒体资源",
                "properties": {
                    "images": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "url": {
                                    "type": "string",
                                    "format": "uri",
                                    "description": "图片原始URL"
                                },
                                "local_path": {
                                    "type": "string",
                                    "description": "本地存储路径"
                                },
                                "title": {
                                    "type": "string",
                                    "description": "图片标题"
                                },
                                "filename": {
                                    "type": "string",
                                    "description": "文件名"
                                }
                            },
                            "required": [
                                "url",
                                "local_path",
                                "title",
                                "filename"
                            ],
                            "additionalProperties": false
                        },
                        "description": "图片列表"
                    }
                },
                "required": [
                    "images"
                ],
                "additionalProperties": false
            },
            "meta": {
                "type": "object",
                "description": "元数据信息",
                "properties": {
                    "name": {
                        "type": "string",
                        "description": "条目名称（包含日期）"
                    },
                    "title": {
                        "type": "string",
                        "description": "页面标题"
                    },
                    "url": {
                        "type": "string",
                        "format": "uri",
                        "description": "原始页面URL"
                    },
                    "listDate": {
                        "type": "string",
                        "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
                        "description": "列表日期（YYYY-MM-DD格式）"
                    }
                },
                "required": [
                    "name",
                    "title",
                    "url",
                    "listDate"
                ],
                "additionalProperties": false
            }
        },
        "required": [
            "assessments",
            "basicInfo",
            "description",
            "keyElements",
            "media",
            "meta"
        ],
        "additionalProperties": false
    },
    "minItems": 1,
    "examples": [
        [
            {
                "assessments": {
                    "contemporaryCharacteristics": [
                        "显著"
                    ],
                    "culturalTradition": [
                        "一般"
                    ],
                    "featureProminence": [
                        "明显"
                    ],
                    "modelRole": [
                        "一般"
                    ]
                },
                "basicInfo": {
                    "officialName": "醉石山庄",
                    "address": "吴中区东山镇槎湾和杨湾之间环山路",
                    "currentUse": "民宿酒店",
                    "area": null,
                    "protectionStatus": "中",
                    "openToPublic": true
                },
                "description": "醉石山庄位于苏州东山杨湾...",
                "keyElements": [
                    "建筑",
                    "花木",
                    "叠山",
                    "理水",
                    "其他"
                ],
                "media": {
                    "images": [
                        {
                            "url": "https://ylj.suzhou.gov.cn/szdoc/szsylj/uploadfile/z100bcddsir.jpg",
                            "local_path": "./images/醉石山庄/1_z100bcddsir.jpg",
                            "title": "image003.jpg",
                            "filename": "1_z100bcddsir.jpg"
                        }
                    ]
                },
                "meta": {
                    "name": "醉石山庄 2024-04-28",
                    "title": "醉石山庄 -\n        苏州市园林局",
                    "url": "https://ylj.suzhou.gov.cn/szsylj/ylml/201912/0377c1a4c10344959205ab69c5163a90.shtml",
                    "listDate": "2024-04-28"
                }
            }
        ]
    ]
}